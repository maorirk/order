<Application x:Class="OrderMonitor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OrderMonitor"
             xmlns:viewModels="clr-namespace:OrderMonitor.ViewModels"
             xmlns:views1="clr-namespace:OrderMonitor.Views"
             Startup="OnStartUp">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:OrderMonitorViewModel}">
            <views1:OrderView></views1:OrderView>
        </DataTemplate>
    </Application.Resources>
</Application>
